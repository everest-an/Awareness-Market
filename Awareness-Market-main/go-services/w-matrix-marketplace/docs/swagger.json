{
    "swagger": "2.0",
    "info": {
        "description": "RESTful API for trading W-Matrix alignment tools and cross-model transformation matrices",
        "title": "W-Matrix Marketplace API",
        "termsOfService": "https://awareness.market/terms",
        "contact": {
            "name": "Awareness Market Support",
            "url": "https://awareness.market/support",
            "email": "support@awareness.market"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "1.0"
    },
    "host": "localhost:8081",
    "basePath": "/api/v1",
    "paths": {
        "/listings": {
            "get": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "Get a list of W-Matrix listings with optional filtering and sorting",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "listings"
                ],
                "summary": "Browse W-Matrix listings",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Filter by source model",
                        "name": "source_model",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by target model",
                        "name": "target_model",
                        "in": "query"
                    },
                    {
                        "type": "number",
                        "description": "Minimum price filter",
                        "name": "min_price",
                        "in": "query"
                    },
                    {
                        "type": "number",
                        "description": "Maximum price filter",
                        "name": "max_price",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Sort by: newest, price_asc, price_desc, rating, sales",
                        "name": "sort_by",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 20,
                        "description": "Number of results",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 0,
                        "description": "Pagination offset",
                        "name": "offset",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of W-Matrix listings",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Invalid request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "Create a new W-Matrix alignment tool listing for trading",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "listings"
                ],
                "summary": "Create a new W-Matrix listing",
                "parameters": [
                    {
                        "description": "Listing details",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.CreateListingRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Listing created successfully",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Invalid request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/purchase": {
            "post": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "Purchase access to a W-Matrix and get a 7-day download URL",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "purchases"
                ],
                "summary": "Purchase a W-Matrix",
                "parameters": [
                    {
                        "description": "Purchase details",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.PurchaseListingRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Purchase successful with download URL",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Invalid request or already purchased",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Listing not found",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.CreateListingRequest": {
            "type": "object",
            "required": [
                "alignment_loss",
                "description",
                "price",
                "source_dim",
                "source_model",
                "target_dim",
                "target_model",
                "title",
                "training_data_size"
            ],
            "properties": {
                "alignment_loss": {
                    "type": "number",
                    "minimum": 0
                },
                "description": {
                    "type": "string"
                },
                "price": {
                    "type": "number",
                    "minimum": 0
                },
                "source_dim": {
                    "type": "integer",
                    "minimum": 1
                },
                "source_model": {
                    "type": "string"
                },
                "target_dim": {
                    "type": "integer",
                    "minimum": 1
                },
                "target_model": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "training_data_size": {
                    "type": "integer",
                    "minimum": 1
                }
            }
        },
        "models.PurchaseListingRequest": {
            "type": "object",
            "required": [
                "listing_id",
                "stripe_payment_intent_id"
            ],
            "properties": {
                "listing_id": {
                    "type": "integer"
                },
                "stripe_payment_intent_id": {
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "ApiKeyAuth": {
            "description": "Enter your API key in the format: Bearer {your_api_key}",
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}